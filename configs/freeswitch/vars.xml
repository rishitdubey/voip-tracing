<include>
  <X-PRE-PROCESS cmd="set" data="default_password=1234"/>
  <X-PRE-PROCESS cmd="set" data="domain=voip-lab.local"/>
  <X-PRE-PROCESS cmd="set" data="bind_server_ip=0.0.0.0"/>
  <X-PRE-PROCESS cmd="set" data="external_rtp_ip=auto"/>
  <X-PRE-PROCESS cmd="set" data="external_sip_ip=auto"/>
  
  <!-- Additional variables for a complete setup -->
  <X-PRE-PROCESS cmd="set" data="domain_name=$${domain}"/>
  <X-PRE-PROCESS cmd="set" data="hold_music=local_stream://moh"/>
  <X-PRE-PROCESS cmd="set" data="use_profile=internal"/>
  <X-PRE-PROCESS cmd="set" data="console_loglevel=info"/>
  <X-PRE-PROCESS cmd="set" data="default_areacode=918"/>
  <X-PRE-PROCESS cmd="set" data="default_country=IN"/>
  
  <X-PRE-PROCESS cmd="set" data="internal_auth_calls=true"/>
  <X-PRE-PROCESS cmd="set" data="internal_sip_port=5060"/>
  <X-PRE-PROCESS cmd="set" data="internal_tls_port=5061"/>
  <X-PRE-PROCESS cmd="set" data="internal_ssl_enable=false"/>
  <X-PRE-PROCESS cmd="set" data="external_auth_calls=false"/>
  <X-PRE-PROCESS cmd="set" data="external_sip_port=5080"/>
  <X-PRE-PROCESS cmd="set" data="external_tls_port=5081"/>
  
  <X-PRE-PROCESS cmd="set" data="rtp_start_port=10000"/>
  <X-PRE-PROCESS cmd="set" data="rtp_end_port=10020"/>
  
  <X-PRE-PROCESS cmd="set" data="global_codec_prefs=OPUS,G722,PCMU,PCMA,GSM"/>
  <X-PRE-PROCESS cmd="set" data="outbound_codec_prefs=OPUS,G722,PCMU,PCMA,GSM"/>
  
  <X-PRE-PROCESS cmd="set" data="sounds_dir">/usr/share/freeswitch/sounds</X-PRE-PROCESS>
  <X-PRE-PROCESS cmd="set" data="conf_dir">/etc/freeswitch</X-PRE-PROCESS>
  <X-PRE-PROCESS cmd="set" data="log_dir">/var/log/freeswitch</X-PRE-PROCESS>
  <X-PRE-PROCESS cmd="set" data="run_dir">/var/run/freeswitch</X-PRE-PROCESS>
  <X-PRE-PROCESS cmd="set" data="temp_dir">/tmp</X-PRE-PROCESS>
  <X-PRE-PROCESS cmd="set" data="data_dir">/var/lib/freeswitch</X-PRE-PROCESS>
  
  <X-PRE-PROCESS cmd="set" data="event_socket_password=ClueCon"/>
  <X-PRE-PROCESS cmd="set" data="core_db_dsn">pgsql://host=database dbname=freeswitch user=freeswitch password=freeswitch_mvp_password</X-PRE-PROCESS>
</include>
